@model GruppAKonsult.ViewModels.ProfileFreelancerViewModel

@{
    ViewBag.Title = "ShowCV";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br>
<h2>CV</h2>
<br />

<div class="row">
    <div class="form-group col-md-2">
        @Html.ActionLink("Visa profil", "UpdatedCV", "ProfileFreelancer", new { id = @Model.Freelancer.Candidate_Id}, new { @class = "btn btn-sm btn-primary"})
    </div>
    
   
    <br>

    <div class="form-group col-md-2">
        <a href='@Url.Action("EmployerFind", "Employer")'>
            <input id="SearchBtn" type='button' class="btn btn-primary btn-lg" value='Tillbaka' />
    </div>
</div>
    </a><br>
<br />
<div class="card border-primary" style="max-width: 80rem;">
    <div class="card-header">Kontaktuppgifter</div>
    <div class="card-body">
        <div class="form-group">
            <label class="col-form-label" for="inputDefault">Förnamn</label><br />
            <h6>@Model.Freelancer.Firstname</h6>
        </div>
        <div class="form-group">
            <label class="col-form-label" for="inputDefault">Efternamn</label><br />
            <h6>@Model.Freelancer.Lastname</h6>
        </div>
        <div class="form-group">
            <label class="col-form-label" for="inputDefault">Nationalitet</label><br />
            <h6>@Model.Freelancer.Nationality</h6>

        </div>
        <div class="form-group">
            <label class="col-form-label" for="inputDefault">Epost</label><br />
            <h6>@Model.Freelancer.Email</h6>

        </div>
        <div class="form-group">
            <label class="col-form-label" for="inputDefault">Telefonnummer</label><br />
            <h6>@Model.Freelancer.Phonenumber</h6>

        </div>
        <div class="form-group">
            <label class="col-form-label" for="inputDefault">Födelsedatum</label><br />
            <h6>@Model.Freelancer.Birthdate</h6>

        </div>
        <div class="form-group">
            <label class="col-form-label" for="inputDefault">Födelseort</label><br />
            <h6>@Model.Freelancer.Cityofbirth</h6>

        </div>
        <div class="form-group">
            <label class="col-form-label" for="inputDefault">Adress</label><br />
            <h6>@Model.Freelancer.Address</h6>

        </div>
        <div class="form-group">
            <label class="col-form-label" for="inputDefault">Postnummer</label><br />

        </div>
        <div class="form-group">
            <label class="col-form-label" for="inputDefault">Stad</label><br />
            <h6>@Model.Freelancer.City</h6>

        </div>
        <div class="form-group">
            <label class="col-form-label" for="inputDefault">Yrke</label><br />
            <h6>@Model.CandidateProfession</h6>

        </div>
        <div class="form-group">
            <label class="col-form-label" for="inputDefault">Kompetens</label><br />
            <h6>@Model.CandidateSkill</h6>

        </div>
        <div class="form-group">
            <label class="col-form-label" for="inputDefault">Språk</label><br />
            <h6>@Model.CandidateLanguage</h6>

        </div>
    </div>
</div>

