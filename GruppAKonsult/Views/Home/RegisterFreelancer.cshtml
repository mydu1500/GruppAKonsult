
@{
    ViewBag.Title = "";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<br>
<h3 id="registerText">Här registrerar du som är frilansare dig</h3>

<form>
    <fieldset>
        <legend>Ange din epost (detta kommer att bli ditt användarnamn) och välj ett säkert lösenord</legend>
        <div class="form-group row">
        </div>
        <div class="form-group">
            <label for="exampleInputEmail1">Email</label>
            <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="frilansare@frilansare.se" required>

        </div>
        <div class="form-group">
            <label for="exampleInputPassword1">Lösenord</label>
            <input type="password" class="form-control" id="exampleInputPassword1" placeholder="*******" required>
        </div>
    </fieldset>
    <a href='@Url.Action("EmptyNewProfile", "ProfileFreelancer")'>
        <input type='button' id="btnRegisterFreelancer2" class="btn btn-primary btn-lg" value='Registrera' />
    </a>
</form>


