@model GruppAKonsult.ViewModels.ProfileFreelancerViewModel
@{
    ViewBag.Title = "UpdatedCV";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@if (Model.Freelancer != null)
{
    @Html.HiddenFor(m=>m.Freelancer.Candidate_Id)


    <h2>CV</h2>
    <br/>
    @*<a href='@Url.Action("ProfileFreelancer", "ProfileFreelancer")'>
        <input id="ShowBtn" type='button' class="btn btn-primary btn-lg" value='Tillbaka till profil'/>
    </a>*@

    @Html.ActionLink("Tillbaka till profil", "ProfileFreelancer", "ProfileFreelancer", new { id = @Model.Freelancer.Candidate_Id}, new { @class = "btn btn-lg btn-primary"})
    <br>
    <br/>

    <div class="card border-primary" style="max-width: 80rem;">
        @*<div class="row">*@

        <div class="card-header">Kontaktuppgifter</div>
        <div class="card-body">
            <div class="form-group">
                <label class="col-form-label" for="inputDefault">Förnamn</label><br />
                <h6 class="card-subtitle text-muted">@Model.Freelancer.Firstname</h6><br>


            </div>
            <div class="form-group">
                <label class="col-form-label" for="inputDefault">Efternamn</label><br />
                <h6 class="card-subtitle text-muted">@Model.Freelancer.Lastname</h6><br>

            </div>

            <div class="form-group">
                <label class="col-form-label" for="inputDefault">Nationalitet</label><br />
                <h6 class="card-subtitle text-muted">@Model.Freelancer.Nationality</h6><br>

            </div>
            <div class="form-group">
                <label class="col-form-label" for="inputDefault">Epost</label><br />
                <h6 class="card-subtitle text-muted">@Model.Freelancer.Email</h6><br>

            </div>
            <div class="form-group">
                <label class="col-form-label" for="inputDefault">Telefonnummer</label><br />
                <h6 class="card-subtitle text-muted">@Model.Freelancer.Phonenumber</h6><br>

            </div>
            <div class="form-group">
                <label class="col-form-label" for="inputDefault">Födelsedatum</label><br />
                <h6 class="card-subtitle text-muted">@Model.Freelancer.Birthdate</h6><br>

            </div>
            <div class="form-group">
                <label class="col-form-label" for="inputDefault">Födelseort</label><br />
                <h6 class="card-subtitle text-muted">@Model.Freelancer.Cityofbirth</h6><br>

            </div>
            <div class="form-group">
                <label class="col-form-label" for="inputDefault">Adress</label><br />
                <h6 class="card-subtitle text-muted">@Model.Freelancer.Address</h6><br>

            </div>
            <div class="form-group">
                <label class="col-form-label" for="inputDefault">Postnummer</label><br />
                <h6 class="card-subtitle text-muted">@Model.Freelancer.Postalnumber</h6><br>

            </div>
            <div class="form-group">
                <label class="col-form-label" for="inputDefault">Stad</label><br />
                <h6 class="card-subtitle text-muted">@Model.Freelancer.City</h6><br>

            </div>
            <div class="form-group">
                <label class="col-form-label" for="inputDefault">Yrke</label><br />
                <h6 class="card-subtitle text-muted">@Model.CandidateSkill</h6><br>

            </div>
            <div class="form-group">
                <label class="col-form-label" for="inputDefault">Kompetens</label><br />
                <h6 class="card-subtitle text-muted">@Model.CandidateProfession</h6><br>
                    

            </div>
            <div class="form-group">
                <label class="col-form-label" for="inputDefault">Språk</label><br />
                <h6 class="card-subtitle text-muted">@Model.CandidateLanguage</h6><br>

            </div>
        </div>
    </div>

    

}