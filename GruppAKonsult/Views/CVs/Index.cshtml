@model IEnumerable<GruppAKonsult.Models.CV>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Drivinglicense)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Education)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Workexperience)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Freelancer.Firstname)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Drivinglicense)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Education)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Workexperience)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Freelancer.Firstname)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.CV_Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.CV_Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.CV_Id })
            </td>
        </tr>

        <th>
            @Html.DisplayNameFor(model => model.Profession)
        </th>
        <td>
            @Html.ListBox("Candidate_Id", new SelectList(item.Profession, "Candidate_Id", "Webdeveloper"))
        </td>
    }

</table>
